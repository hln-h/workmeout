import React from "react";
import { Outlet, Link } from "react-router-dom";
import database from "../database.json";
​
export default function Users() {
  return (
    <div>
      <h1>This is my user list</h1>
      {database.map((user) => (
        <div>
          <Link to={`/users/${user.id}`}>{user.name}</Link>
        </div>
      ))}
​
      <hr />
​
      <Outlet />
    </div>
  );
}